  "semanticHighlighting": true,
  "semanticTokenColors": {
    "annotation": {
      "foreground": "#b3ae60"
    },
    "annotation.declaration": {
      "foreground": "#b3ae60"
    },
    "*.annotation": {
      "foreground": "#b3ae60"
    },
    "annotationMember": {
      "foreground": "#bcbec4"
    },
    "property.annotation": {
      "foreground": "#bcbec4"
    },
    "parameter": {
      "foreground": "#bcbec4"
    },
    "parameter.declaration": {
      "foreground": "#bcbec4"
    },
    "variable": {
      "foreground": "#bcbec4"
    },
    "variable.readonly": {
      "foreground": "#c77dbb"
    },
    "variable.defaultLibrary": {
      "foreground": "#c77dbb",
      "fontStyle": "italic"
    },
    "property": {
      "foreground": "#c77dbb"
    },
    "property.readonly": {
      "foreground": "#c77dbb"
    },
    "property.defaultLibrary": {
      "foreground": "#14c4b6"
    },
    "method": {
      "foreground": "#bcbec4"
    },
    "method.declaration": {
      "foreground": "#56a8f5"
    },
    "method.static": {
      "foreground": "#bcbec4"
    },
    "method.static.declaration": {
      "foreground": "#56a8f5",
      "fontStyle": "italic"
    },
    "function": {
      "foreground": "#bcbec4"
    },
    "function.declaration": {
      "foreground": "#56a8f5"
    },
    "typeParameter": {
      "foreground": "#16baac"
    },
    "type": {
      "foreground": "#bcbec4"
    },
    "interface": {
      "foreground": "#bcbec4"
    },
    "class": {
      "foreground": "#bcbec4"
    },
    "enum": {
      "foreground": "#bcbec4"
    },
    "enumMember": {
      "foreground": "#c77dbb",
      "fontStyle": "italic"
    },
    "namespace": {
      "foreground": "#c77dbb",
      "fontStyle": "italic"
    }
  },
  "tokenColors": [
    {
      "name": "Punctuation (commas, semicolons, etc)",
      "scope": [
        "punctuation.separator.comma",
        "punctuation.separator.delimiter",
        "punctuation.terminator.statement",
        "meta.brace.round",
        "meta.brace.square",
        "punctuation.definition.parameters",
        "punctuation.separator.parameter"
      ],
      "settings": {
        "foreground": "#bcbec4"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#7a7e85"
      }
    },
    {
      "name": "Block Comments",
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#5f826b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Block Comments - type",
      "scope": ["comment.block.documentation storage.type"],
      "settings": {
        "foreground": "#67a37c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["source.css.scss comment"],
      "settings": {
        "foreground": "#b9954e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "keyword.operator.expression",
        "storage.modifier",
        "constant.language",
        "variable.language.this",
        "variable.key.dotenv",
        "entity.name.command",
        "entity.name.tag.yaml",
        "constant.character.escape",
        "punctuation.definition.markdown",
        "punctuation.definition.raw.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.quote.begin.markdown"
      ],
      "settings": {
        "foreground": "#cf8e6d",
        "fontStyle": ""
      }
    },
    {
      "name": "Storage Type (should be white for variable declarations)",
      "scope": [
        "storage.type.java",
        "storage.type.primitive.java",
        "storage.type.object.array.java",
        "storage.type.generic.java"
      ],
      "settings": {
        "foreground": "#bcbec4"
      }
    },
    {
      "name": "Class and Type References",
      "scope": [
        "support.type",
        "support.class",
        "variable.other.class",
        "entity.other.inherited-class",
        "meta.import.java",
        "entity.name.type.class.java",
        "entity.name.type.interface.java",
        "meta.class.identifier.java entity.name.type"
      ],
      "settings": {
        "foreground": "#bcbec4"
      }
    },
    {
      "name": "Method and Function Calls (should be white)",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "meta.method-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#bcbec4"
      }
    },
    {
      "name": "Method Declaration (should be blue, more specific)",
      "scope": [
        "meta.method.identifier.java",
        "meta.method.java entity.name.function",
        "meta.method.declaration.java entity.name.function"
      ],
      "settings": {
        "foreground": "#56a8f5"
      }
    },
    {
      "name": "Storage (class, function, etc keywords)",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#cf8e6d"
      }
    },
    {
      "scope": ["punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "#ce8e6d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.type.module",
        "entity.name.constant",
        "variable.other.object",
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.constant.property",
        "variable.other.enummember",
        "variable.language",
        "meta.object-literal.key",
        "meta.definition.property",
        "meta.definition.property entity.name.function",
        "support.type.property-name.json",
        "support.variable.property",
        "support.type.object.module",
        "fenced_code"
      ],
      "settings": {
        "foreground": "#c77dbb",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.heading"],
